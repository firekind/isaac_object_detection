{
    "name": "viewers_subgraph",
    "modules": [
        "sight",
        "viewers"
    ],
    "graph": {
        "nodes": [
            {
                "name": "subgraph",
                "components": [
                    {
                        "name": "ledger",
                        "type": "isaac::alice::MessageLedger"
                    },
                    {
                        "name":  "interface",
                        "type": "isaac::alice::Subgraph"
                    }
                ]
            },
            {
                "name": "edge_detection_viewer",
                "components": [
                    {
                        "name": "ledger",
                        "type": "isaac::alice::MessageLedger"
                    },
                    {
                        "name": "viewer",
                        "type": "isaac::viewers::ImageViewer"
                    }
                ]
            },
            {
                "name": "object_detection_viewer",
                "components": [
                    {
                        "name": "ledger",
                        "type": "isaac::alice::MessageLedger"
                    },
                    {
                        "name": "viewer",
                        "type": "isaac::viewers::DetectionsViewer"
                    }
                ]
            },
            {
                "name": "scene_viewer",
                "components": [
                    {
                        "name": "message_ledger",
                        "type": "isaac::alice::MessageLedger"
                    },
                    {
                        "name": "viewer",
                        "type": "isaac::viewers::ImageViewer"
                    }
                ]
            },
            {
                "name": "sight_widgets",
                "components": [
                    {
                        "name": "edges",
                        "type": "isaac::sight::SightWidget"
                    },
                    {
                        "name": "scene",
                        "type": "isaac::sight::SightWidget"
                    },
                    {
                        "name": "detections",
                        "type": "isaac::sight::SightWidget"
                    }
                ]
            }
        ],
        "edges": [
            {
                "source": "subgraph/interface/edges",
                "target": "edge_detection_viewer/viewer/image"
            },
            {
                "source": "subgraph/interface/detections",
                "target": "object_detection_viewer/viewer/detections"
            },
            {
                "source": "subgraph/interface/input_image",
                "target": "scene_viewer/viewer/image"
            }
        ]
    },
    "config": {
        "scene_viewer": {
            "viewer": {
                "camera_name": "camera"
            }
        },
        "edge_detection_viewer": {
            "viewer": {
                "target_fps": 20
            }
        },
        "websight": {
            "WebsightServer": {
                "port": 3000
            }
        },
        "sight_widgets": {
            "detections": {
                "type": "2d",
                "channels": [
                    {
                        "name": "$(fullname scene_viewer/viewer/image)"
                    },
                    {
                        "name": "$(fullname object_detection_viewer/viewer/detections)"
                    }
                ]
            },
            "edges": {
                "type": "2d",
                "channels": [
                    {
                        "name": "$(fullname edge_detection_viewer/viewer/image)"
                    }
                ]
            },
            "scene": {
                "type": "2d",
                "channels": [
                    {
                        "name": "$(fullname scene_viewer/viewer/image)"
                    }
                ]
            }
        }
    }
}