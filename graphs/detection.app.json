            {
                "name": "camera",
                "components": [
                    {
                        "name": "ledger",
                        "type": "isaac::alice::MessageLedger"
                    },
                    {
                        "name": "v4l2_camera",
                        "type": "isaac::V4L2Camera"
                    }
                ]
            },
            {
                "name": "object_detector",
                "subgraph": "apps/isaac_object_detection/graphs/subgraphs/object_detection.subgraph.json"
